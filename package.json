{
  "name": "@crcdevops/open-source-design-system",
  "version": "1.0.5",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.14",
    "@fortawesome/free-solid-svg-icons": "^5.7.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@storybook/addon-notes": "5.2.4",
    "@storybook/addon-options": "5.2.4",
    "@storybook/addons": "5.2.4",
    "@storybook/react": "5.2.5",
    "ag-grid-community": "^20.0.0",
    "ag-grid-react": "^20.0.0",
    "codecov": "^3.1.0",
    "formik": "^1.4.1",
    "jest": "^24.8.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-datepicker": "^2.1.0",
    "react-dom": "^16.6.3",
    "react-modal": "^3.8.1",
    "react-scripts": "^3.0.1",
    "react-select": "^2.1.2",
    "react-tooltip": "^3.9.2",
    "styled-components": "^4.3.1",
    "wait-on": "^3.2.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "clean-dist": "rm -rf ./dist",
    "start": "react-scripts start",
    "build": "yarn run clean-dist && babel src -d dist --ignore '**/*.test.js','**/*.stories.js' && cp src/index.css dist/index.css && cp -a src/assets dist/assets",
    "lint": "node_modules/eslint/bin/eslint.js 'src' --ext .js,.jsx",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "git add"
    ]
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "jest": {
    "setupFiles": [
      "./jestSetup.js",
      "./jestOverrides.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.js",
      ".+\\.(png|jpg|ttf|woff|svg)$": "identity-obj-proxy"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 98.3,
        "branches": 93.4,
        "functions": 94.9,
        "lines": 98.8
      }
    },
    "setupFilesAfterEnv": [
      "./setupTest.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-info": "^5.3.6",
    "@storybook/addon-knobs": "5.1.11",
    "@storybook/addon-links": "^5.1.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.8.0",
    "cypress": "^3.4.1",
    "cypress-cucumber-preprocessor": "^1.16.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^9.0.0",
    "prettier": "^1.15.3"
  },
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Client-Relationship-Consultancy/open-source-design-system.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Client-Relationship-Consultancy/open-source-design-system/issues"
  },
  "homepage": "https://github.com/Client-Relationship-Consultancy/open-source-design-system#readme"
}
