{
  "parser": "@typescript-eslint/parser",
  "extends": ["airbnb", "prettier", "prettier/react", "prettier/@typescript-eslint","plugin:@typescript-eslint/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "plugins": ["react", "jsx-a11y", "prettier", "@typescript-eslint", "import"],
  "parserOptions": {Removes tslint, includes typescript-eslint/parser, adds old rules
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "@typescript-eslint/camelcase": 0,
    "@typescript-eslint/interface-name-prefix": 0,
    "@typescript-eslint/interface-name": 0,
    "@typescript-eslint/ban-ts-ignore": 0,
    "@typescript-eslint/jsx-no-multiline-js": 0,
    "@typescript-eslint/jsx-boolean-value": 0,
    "@typescript-eslint/member-access": 0,
    "@typescript-eslint/object-literal-sort-keys": 0,
    "@typescript-eslint/no-explicit-any": 0,
    "@typescript-eslint/explicit-function-return-type": 0,
    "@typescript-eslint/no-empty-function": 0,
    "quotes": [2, "double", { "avoidEscape": true }],
    "class-methods-use-this": 0,
    "import/imports-first": 0,
    "import/no-dynamic-require": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-named-as-default": 0,
    "import/no-webpack-loader-syntax": 0,
    "import/prefer-default-export": 0,
    "jsx-a11y/aria-props": 2,
    "jsx-a11y/label-has-for": 0,
    "jsx-a11y/mouse-events-have-key-events": 2,
    "jsx-a11y/role-has-required-aria-props": 2,
    "jsx-a11y/role-supports-aria-props": 2,
    "no-console": 1,
    "no-underscore-dangle": 0,
    "prefer-template": 2,
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0,
    "react/require-extension": 0,
    "react/jsx-filename-extension": [2, { "extensions": [".js", ".ts",".tsx"] }],
    "react/jsx-one-expression-per-line": 0,
    "react/destructuring-assignment": 0,
    "require-yield": 0,
    "react/prefer-stateless-function": 0,
    "linebreak-style": 0,
    "react/jsx-props-no-spreading": 0,
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".tsx", ".ts"]
      }
    }
  },
  "overrides": [{
    "files": "**/*.ts*",
    "rules": {
      "no-undef": "off",
      "react/forbid-prop-types": 1,
      "react/prop-types": "off"
    }
  }]
}